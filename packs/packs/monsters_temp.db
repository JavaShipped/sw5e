e</p>","public":""},"alignment":"unaligned","species":"","type":{"value":"custom","subtype":"","swarm":"","custom":"beast"},"environment":"","cr":0.25,"powerForceLevel":0,"powerTechLevel":0,"xp":{"value":10},"source":"SnV"},"traits":{"size":"sm","di":{"value":[],"custom":""},"dr":{"value":[],"custom":""},"dv":{"value":[],"custom":""},"ci":{"value":[],"custom":""},"languages":{"value":[],"custom":""}},"currency":{"gc":0},"skills":{"acr":{"value":0,"ability":"dex","bonuses":{"check":"","passive":""}},"ani":{"value":0,"ability":"wis","bonuses":{"check":"","passive":""}},"ath":{"value":0,"ability":"str","bonuses":{"check":"","passive":""}},"dec":{"value":0,"ability":"cha","bonuses":{"check":"","passive":""}},"ins":{"value":0,"ability":"wis","bonuses":{"check":"","passive":""}},"itm":{"value":0,"ability":"cha","bonuses":{"check":"","passive":""}},"inv":{"value":0,"ability":"int","bonuses":{"check":"","passive":""}},"lor":{"value":0,"ability":"int","bonuses":{"check":"","passive":""}},"med":{"value":0,"ability":"wis","bonuses":{"check":"","passive":""}},"nat":{"value":0,"ability":"int","bonuses":{"check":"","passive":""}},"pil":{"value":0,"ability":"int","bonuses":{"check":"","passive":""}},"prc":{"value":0,"ability":"wis","bonuses":{"check":"","passive":""},"proficient":2},"prf":{"value":0,"ability":"cha","bonuses":{"check":"","passive":""}},"per":{"value":0,"ability":"cha","bonuses":{"check":"","passive":""}},"slt":{"value":0,"ability":"dex","bonuses":{"check":"","passive":""}},"ste":{"value":0,"ability":"dex","bonuses":{"check":"","passive":""}},"sur":{"value":0,"ability":"wis","bonuses":{"check":"","passive":""}},"tec":{"value":0,"ability":"int","bonuses":{"check":"","passive":""}}},"powers":{"power1":{"fvalue":1000,"fmax":0,"foverride":null,"tvalue":1000,"tmax":0,"toverride":null},"power2":{"fvalue":1000,"fmax":0,"foverride":null,"tvalue":1000,"tmax":0,"toverride":null},"power3":{"fvalue":1000,"fmax":0,"foverride":null,"tvalue":1000,"tmax":0,"toverride":null},"power4